                            this.videoplyr.quality = availableQualities[window.hls.currentLevel];
                        }
                    });
         
                    videoplyr.on('waiting', event => {
                    const startBuffer = videoplyr.buffered;
                    setTimeout(() => {
                        //console.log('Player Buffering');
                        //console.log({
                        //startBuffer,
